/* pages/other/index.wxss */
.icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}

.title {
  width: 160rpx;
  height: 45rpx;
  display: block;
  overflow-wrap: break-word;
  color: rgba(51,51,51,1);
  font-size: 32rpx;
  font-family: PingFangSC-Medium;
  white-space: nowrap;
  line-height: 45rpx;
  text-align: left;
}

.size{
  width: 193rpx;
  height: 33rpx;
  display: block;
  overflow-wrap: break-word;
  color: rgba(153,153,153,1);
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  white-space: nowrap;
  line-height: 33rpx;
  text-align: left;
  margin: 9rpx 0 0 20rpx;
}
.header{
  width: 100%;
}
.pos {
  position: relative;
  left: 80rpx
}
.pos1{
  position: relative;
  left: 50rpx;
  top: 5rpx;
}
.pos3{
  position: relative;
  left: 50rpx;
}
.imageWrapper{
  width: 100%;
  align-items: flex-end;
  display: flex;
  flex-wrap: wrap;
  margin-top: 40rpx;
}
.image{
  width: 100%;
  border: 1rpx solid #aaa;
  max-height: 400rpx;
  text-align: center;
}
.wrapper{
  width: 80%;
}
.line{
  width: 100%;    /* 分隔线的长度 */
  margin: auto;
  height: 1px;    /* 分隔线粗细 */
  text-align: center;
  font-size: 24rpx;
  color: #aaa;
  background: #ccc;
  margin-top: 20px;
}
.cameradd{
  font-size: 300rpx;
  color: #aaa;
  text-align: center;
}